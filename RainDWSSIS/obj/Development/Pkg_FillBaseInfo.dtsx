                    sw.WriteLine($"Error Code    : {errorCode}");
                    sw.WriteLine($"Error Message : {errorDesc}");
                    sw.WriteLine("--------------------------------------------------");
                    sw.WriteLine();
                }

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
