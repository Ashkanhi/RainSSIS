                string logFolderPath = Dts.Variables["User::VR_LogFolderPath"].Value.ToString();

                // üîπ ÿ®ÿ±ÿ±ÿ≥€å Ÿàÿ¨ŸàÿØ ŸÅŸàŸÑÿØÿ±
                if (!Directory.Exists(logFolderPath))
                    Directory.CreateDirectory(logFolderPath);

                // üîπ ÿÆŸàÿßŸÜÿØŸÜ ŸÖÿ™ÿ∫€åÿ±Ÿáÿß€å ÿ≥€åÿ≥ÿ™ŸÖ€å
                string packageName = Dts.Variables["System::PackageName"].Value.ToString();
                string sourceTask = Dts.Variables["System::SourceName"].Value.ToString(); // Task ÿßÿµŸÑ€å
                string scriptTask = Dts.Variables["System::TaskName"].Value.ToString();   // ÿÆŸàÿØ Script Task
                string executionId = Dts.Variables["System::ExecutionInstanceGUID"].Value.ToString();
                string status = "Success"; // ⁄ÜŸàŸÜ PostExecute ÿßÿ¨ÿ±ÿß ÿ¥ÿØŸáÿå €åÿπŸÜ€å ŸÖŸàŸÅŸÇ ÿ®ŸàÿØŸá

